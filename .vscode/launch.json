{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug API (FastAPI)",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app:app",
                "--reload",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "QUEUE_DIR": "${workspaceFolder}/rosreestr_queue",
                "OUTPUT_DIR": "${workspaceFolder}/output"
            }
        },
        {
            "name": "Debug Worker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/worker.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "QUEUE_DIR": "${workspaceFolder}/rosreestr_queue",
                "OUTPUT_DIR": "${workspaceFolder}/output",
                // "PROXY_HOST": "gw.proxys.io",  
